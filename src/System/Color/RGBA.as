package System.Color 
{
	/**
	 * ...
	 * @author dorofiy
	 */
	public class RGBA 
	{
		public var red:int;
		public var green:int;
		public var blue:int;
		public var alpha:int;
		
		public function RGBA(red:int = 0, green:int = 0, blue:int = 0, alpha:int = 0) 
		{
			this.red = red;
			this.green = green;
			this.blue = blue;
			this.alpha = alpha;
		}
	}

}